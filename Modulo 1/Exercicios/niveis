nome = input("Qual √© o seu nome, sinh√¥?: ")
nivel_humor = int(input("Qual √© o seu n√¨vel de humor hoje?: "))

if nivel_humor <= 19:
    print(f"{nome} seu nivel de humor est√° exausto üò´")
elif nivel_humor <= 39:
    print(f"{nome} seu nivel de humor est√° cansado ü•±")
elif nivel_humor <= 59:
    print(f"{nome} seu nivel de humor est√° neutro üòê")
elif nivel_humor <= 79:
    print(f"{nome} seu nivel de humor est√° animado üòÄ")
elif nivel_humor <= 100:
    print(f"{nome} seu nivel de humor est√° euforico üòÅ")
else:
    print(f"{nome} seu nivel de falicidade est√° invalida")


